<?xml version="1.0" encoding="UTF-8"?>
<jnlp spec="1.0+" 
      href="pado-desktop.jnlp">
    <information>
        <title>Pado Desktop</title>
        <vendor>Netcrest Technologies, LLC</vendor>
        <homepage href="desktop/help-desktop.html"/>
	    <description>PADO Desktop on Web</description>
	    <description kind="short">PADO GUI tool for monitoring and controlling data grids</description>
	    <icon href="../images/pado_64x64.jpg"/>
	    <icon kind="splash" href="../images/pado_logo.jpg"/>
        <offline-allowed/> 
        <association mime-type="application-x/pado-desktop-file"  extensions="desktop"/> 
        <shortcut online="true" install="true">
	      <desktop/>
	      <menu submenu="Pado"/>
	    </shortcut>
    </information>
    <security>
		<all-permissions/>
	</security>

    <resources>

