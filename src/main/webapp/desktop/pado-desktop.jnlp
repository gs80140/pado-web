 		<jar href="slib/JHyperlink.jar"/>
		<jar href="slib/activation.jar"/>
		<jar href="slib/commons-codec-1.9.jar"/>
		<jar href="slib/gemfire.jar"/>
		<jar href="slib/antlr.jar"/>
		<jar href="slib/gfsh-dependencies.jar"/>
		<jar href="slib/log4j-api-2.1.jar"/>
		<jar href="slib/log4j-core-2.1.jar"/>
		<jar href="slib/lucene-queryparser-4.7.2.jar"/>
		<jar href="slib/org-netbeans-swing-outline.jar"/>
		<jar href="slib/pado-common.jar"/>
		<jar href="slib/pado-core.jar"/>
		<jar href="slib/pado-gemfire.jar"/>
		<jar href="slib/pado-biz.jar"/>
		<jar href="slib/pado-biz-gemfire.jar"/>
		<jar href="slib/pado-desktop.jar"/>
		<jar href="slib/pado-test.jar"/>
		<jar href="slib/swingx-all-1.6.3.jar"/>
		<jar href="slib/swingx-beaninfo-1.6.3.jar"/>
		<jar href="slib/naf/infocom.jar"/>
		<jar href="slib/naf/javax.jar"/>
		<jar href="slib/naf/jms1_1.jar"/>
